{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "ExampleA-route": {
        "ClusterId": "ExampleA-service",
        "Match": {
          "Path": "/ExampleA/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/ExampleA"
          }
        ]
      },
      "ExampleB-route": {
        "ClusterId": "ExampleB-service",
        "Match": {
          "Path": "/ExampleB/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/ExampleB"
          }
        ]
      }
    },
    "Clusters": {
      "ExampleA-service": {
        "Destinations": {
          "ExampleA/instance1": {
            //"Address": "http://ExampleA-service"
            //"Address": "http://localhost:62613/v1.0/invoke/ExampleA-service/method/"
            "Address": "http://localhost:60123/v1.0/invoke/ExampleA-service/method/"
          }
        }
      },
      "ExampleB-service": {
        "Destinations": {
          "ExampleB/instance1": {
            //"Address": "http://ExampleA-service"
            //"Address": "http://localhost:62613/v1.0/invoke/ExampleA-service/method/"
            "Address": "http://localhost:60123/v1.0/invoke/ExampleB-service/method/"
          }
        }
      }
    }
  },
  "AllowedHosts": "*"
}
